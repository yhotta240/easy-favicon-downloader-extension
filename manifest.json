{
  "manifest_version": 3,
  "name": "簡単ファビコンダウンロード - Easy Favicon Downloader",
  "version": "1.0.1",
  "description": "WEBサイトのファビコン（Favicon）を簡単にダウンロードできます",
  "icons": {
    "48": "icons/icon.png",
    "128": "icons/icon.png"
  },
  "permissions": ["storage", "activeTab"],
  "action": {
    "default_popup": "popup/popup.html"
  },
  "host_permissions": ["<all_urls>"]
}
